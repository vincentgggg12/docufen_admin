{
  "name": "docufen_admin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Internal admin dashboard for Docufen customer management and metrics",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:local": "LOCAL=true vite preview",
    "test": "playwright test",
    "test:local": "BASE_URL=https://localhost:3030 playwright test --ui",
    "test:stage": "BASE_URL=https://stage.docufen.com npx playwright test --ui",
    "test:prod": "BASE_URL=https://app.docufen.com npx playwright test --ui",
    "test:ts": "playwright test --config=playwright.config.ts-reporter.ts",
    "test:ts:7.1": "playwright test 7_DocumentCompletion/7.1.CreateNewDocument/TS.7.1-01-CreateDocumentDialogDisplay.spec.ts --config=playwright.config.ts-reporter.ts",
    "test:ts:7.1-02": "playwright test 7_DocumentCompletion/7.1.CreateNewDocument/TS.7.1-02-DocumentNameInputValidation.spec.ts --config=playwright.config.ts-reporter.ts",
    "test:validate": "ts-node playwright/tests/utils/validate-configs.ts",
    "test:migrate": "node playwright/tests/utils/migrate-to-v2.js",
    "codegen:local": "playwright codegen --viewport-size=1920,1080 https://localhost:3030",
    "codegen:beta": "playwright codegen --viewport-size=1920,1080 https://beta.docufen.com",
    "codegen:stage": "playwright codegen --viewport-size=1920,1080 https://stage.docufen.com",
    "debug:generate": "node scripts/generate-debug-swahili.js"
  },
